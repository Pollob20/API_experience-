curl -X POST \
  -H "Accept: application/vnd.github+json" \
  -H "Authorization: Bearer YOUR_GITHUB_TOKEN" \
  https://api.github.com/repos/pollob123/my-workflow-demo/actions/workflows/build.yml/dispatches \
  -d '{
    "ref": "main",
    "inputs": {
      "param1": "Hello World"
    },
    "return_run_details": true
  }'
